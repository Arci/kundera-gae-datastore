<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             version="1.0" xmlns="http://java.sun.com/xml/ns/persistence"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="pu">
        <provider>com.impetus.kundera.KunderaPersistence</provider>
        <class>it.polimi.datastore.test.model.Address</class>
        <class>it.polimi.datastore.test.model.Department</class>
        <class>it.polimi.datastore.test.model.DepartmentOTM</class>
        <class>it.polimi.datastore.test.model.Employee</class>
        <class>it.polimi.datastore.test.model.EmployeeEmbedded</class>
        <class>it.polimi.datastore.test.model.EmployeeMTM</class>
        <class>it.polimi.datastore.test.model.EmployeeMTO</class>
        <class>it.polimi.datastore.test.model.EmployeeMTObis</class>
        <class>it.polimi.datastore.test.model.EmployeeOTO</class>
        <class>it.polimi.datastore.test.model.Phone</class>
        <class>it.polimi.datastore.test.model.PhoneEnum</class>
        <class>it.polimi.datastore.test.model.PhoneOTO</class>
        <class>it.polimi.datastore.test.model.PhoneType</class>
        <class>it.polimi.datastore.test.model.Project</class>
        <class>it.polimi.datastore.test.model.ProjectMTM</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties>
            <property name="kundera.keyspace" value="gae-test"/>
            <property name="kundera.client.lookup.class" value="it.polimi.client.datastore.DatastoreClientFactory"/>
        </properties>
    </persistence-unit>
</persistence>